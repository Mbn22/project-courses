<%- include('header'); -%> 


<h2 class="text-dark">User</h2>   

<table class="ui celled padded table">
    <thead>
      <tr><th class="single line">Email</th>
      <th>Name</th>
      <th>Profile</th>
    </tr></thead>
    <tbody>
      <% if (users) {for(var i=0;i<users.length; i++){ %>
        <tr>
          <td>
            <h3 class="ui center aligned header"><%= users[i].email %></h3>
          </td>
          <td class="single line">
            <%= users[i].firstName %> <%= users[i].lastName %>
          </td>
          <td> <strong><%= users[i].profile %></td></strong>
         
        
         <% } } %>
    </tbody>
    <tfoot>
      <tr><th colspan="5">
        <div class="ui right floated pagination menu">
          <a class="icon item">
            <i class="left chevron icon"></i>
          </a>
          <a class="item">1</a>
          <a class="item">2</a>
          <a class="item">3</a>
          <a class="item">4</a>
          <a class="icon item">
            <i class="right chevron icon"></i>
          </a>
        </div>
      </th>
    </tr></tfoot>
  </table>


</p>
</div>
</body>
<%- include('_footer') %>